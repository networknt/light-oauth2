{"components":{"securitySchemes": {
      "key_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://localhost:8888/oauth2/code",
            "scopes": {
              "oauth.key.w": "write key",
              "oauth.key.r": "read key"
            }
          }
        }
      }
    },"schemas":{"Key":{"type": "object","required":["keyId",
          "certificate"],"properties":{"certificate":{"type": "string","description": "certificate"},"keyId":{"type": "string","description": "a unique id"}}}}},"servers": [
    {
      "url": "https://oauth2.networknt.com"
    }
  ],"openapi": "3.0.0","paths": {
    "/oauth2/key/{keyId}": {
      "get": {
        "description": "Get a key by Id",
        "operationId": "getKeyById",
        "parameters": [
          {
            "name": "keyId",
            "in": "path",
            "description": "Key Id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Key"
                }
              }
            }
          },
          "400": {
            "description": "Invalid keyId supplied"
          },
          "404": {
            "description": "Key not found"
          }
        },
        "security": [
          {
            "key_auth": [
              "oauth.key.r",
              "oauth.key.w"
            ]
          }
        ]
      }
    }
  },"info": {
    "version": "1.0.0",
    "title": "OAuth2 Key Service",
    "description": "OAuth2 Key Service microservices endpoints.",
    "contact": {
      "email": "stevehu@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  }}